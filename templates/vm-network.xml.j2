<network>
  <name>{{ item.key }}</name>
  <bridge name="{{ item.key }}"/>
  <forward mode='nat'>
    <nat>
      <port start='1024' end='65535'/>
    </nat>
  </forward>
  <ip address="{{ item.value.router }}" netmask="{{ item.value.netmask }}">
    <dhcp>
      <range start="{{ item.value.start }}" end="{{ item.value.end }}"/>
    </dhcp>
  </ip>
</network>

